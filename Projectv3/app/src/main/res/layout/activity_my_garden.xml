<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundtree"
    tools:context="com.example.ss.deadlineslul.Settings">

    <Button
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:onClick="Prev"
        android:background="@drawable/button_bg_round_corners"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textSize="20sp"
        android:textColor="#000000"
        android:text="Пред"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        tools:ignore="ContentDescription"
        />


    <Button
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:background="@drawable/button_bg_round_corners"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="20sp"
        android:textColor="#000000"
        android:text="След"
        app:layout_constraintVertical_bias="0.02"
        app:layout_constraintHorizontal_bias="0.95"
        tools:ignore="ContentDescription"
        android:onClick="Next"/>


    <TextView
        android:id="@+id/textViewfirst"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="18 Неделя"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintVertical_bias="0.025" />


    <TextView
        android:id="@+id/textViewsecond"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:gravity="center_horizontal"
        android:text="01.05.2018"
        android:textSize="15sp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />




    <ListView
        android:layout_width="400dp"
        android:id="@+id/List"
        android:layout_height="500dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.55"
        app:layout_constraintHorizontal_bias="0.5">
    </ListView>


    <Button
        android:text="Материалы"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.985"
        app:layout_constraintHorizontal_bias="0.5"
        android:background="@drawable/button_bg_round_corners"
        android:textColor="#000000"
        android:textSize="20sp"
        android:onClick="Intenthere"/>








</android.support.constraint.ConstraintLayout>
